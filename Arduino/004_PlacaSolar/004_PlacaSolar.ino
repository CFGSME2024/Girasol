//********** Includes *************************************************************

//********** Variables ************************************************************
float Tensio=0;
const int Placa=A2;
int Valor_Placa;

//********** Setup ****************************************************************
void setup() 
{
  pinMode (Placa,INPUT);
  


}

//********** Loop *****************************************************************
void loop() 
{
  Valor_Placa=analogRead(Placa);
  Tensio=(Valor_Placa*5.0)/1023;
  Serial.print(Tensio);

}

